<body class="jumbo-page" ng-app="InstaAlbumModule">
    <main class="admin-main  ">
        <div class="container-fluid">
            <div class="row ">
                <div class="col-lg-4  bg-white"  ng-controller="LoginController">
                    <div class="row align-items-center m-h-100">
                        <div class="mx-auto col-md-8">
                            <div class="p-b-20 text-center">
                                <p>
                                    <img src="~/assets/images/picticklogo.png" width="150" alt="">
                                </p>
                                <p class="admin-brand-content">
                                    Pictik
                                </p>
                            </div>
                            <h3 class="text-center p-b-20 fw-400">Login</h3>
                            <form ng-submit="LoginAdmin()">
                                <div class="form-row">
                                    <div class="form-group floating-label col-md-12">
                                        <label>UserName</label>
                                        <input type="text" class="form-control" ng-model="UserName" placeholder="User Name" name="UserName" required="required">
                                    </div>
                                    <div class="form-group floating-label col-md-12">
                                        <label>Password</label>
                                        <input type="password" class="form-control" ng-model="Password" placeholder="Password" name="password" required="required">
                                    </div>
                                </div>
                                <button type="submit" class="btn btn-primary btn-block btn-lg">Login</button><br />
                                <a href="~/StudioLogin" class="btn btn-primary btn-block btn-lg">
                                Studio Login</a>
                            </form>
                            <p class="text-right p-t-10">
                                
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-8 d-none d-md-block bg-cover" style="background-image: url('../../assets/images/backgrounds/adminback.jpg');">
                </div>
            </div>
        </div>
    </main>
    
    
